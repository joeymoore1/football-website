GET     /                           controllers.HomeController.index()

GET     /team                       controllers.TeamController.team()

GET     /fixtures                   controllers.FixturesController.index()

GET     /results                    controllers.ResultsController.index()

GET     /profile/:id                controllers.ProfileController.index(id: Int)

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
